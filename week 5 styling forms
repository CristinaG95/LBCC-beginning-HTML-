*CSS2*

@import url("https://fonts.googleapis.com/css2?family=Comfortaa:wght@300..700&family=Exo+2:ital,wght@0,100..900;1,100..900&display=swap");

h1 {
  font-family: "Exo 2";
}
h2 {
  color: white;
  font-family: "Comfortaa";
  margin-left: 30px;
  padding-top: 30px;
}
main {
  background: rgb(131, 58, 180);
  background: linear-gradient(
    317deg,
    rgba(131, 58, 180, 1) 25%,
    rgba(253, 29, 29, 1) 50%,
    rgba(252, 176, 69, 1) 75%
  );
  background-attachment: fixed;
  background-size: cover;
  background-repeat: no-repeat;
  border-radius: 5px;
  width: 600px;
  height: 610px;
}
li {
  padding: 10px;
  font-size: 20px;
  margin: 10px;
  background-color: rebeccapurple;
}
li:hover {
  background-color: lavender;
}
form {
  margin-left: 170px;
  width: 210px;
  float: left;
}

input[type="text"],
input[type="email"],
input[type="number"],
select {
  background-color: rgba(0, 0, 0, 0);
  border: 1px solid white;
  border-radius: 30px;
  color: white;
  display: block;
  margin: 0 0 5px 0;
  padding: 15px 25px;
  width: 200px;
}

::placeholder {
  color: white;
}

input[type="submit"] {
  background-color: orange;
  border: 1px solid orange;
  border-radius: 30px;
  padding: 10px 25px;
  width: 200px;
}

input[type="submit"],
select {
  box-sizing: content-box;
}
