*CSS1*
@import url("https://fonts.googleapis.com/css2?family=Comfortaa:wght@300..700&family=Exo+2:ital,wght@0,100..900;1,100..900&display=swap");

h1 {
  font-family: "Exo 2";
  margin-left: 40px;
}
h2 {
  font-family: "Comfortaa";
  margin-left: 10px;
}
nav {
  font-family: "Exo 2";
  padding: 10px;
}
li {
  padding: 10px;
  font-size: 20px;
  margin: 10;
  background-color: rebeccapurple;
}
li:hover {
  background-color: lavender;
}
.form-column {
  display: inline-block;
  width: 45%;
}
div {
  margin: 5px 10px;
  font-family: "Comfortaa";
}
input[type="text"],
input[type="email"],
input[type="number"],
select {
  border: 1px solid black;
  border-radius: 3px;
  padding: 6px 5px 15px;
  width: 90%;
}
input[type="submit"] {
  font-family: "Comfortaa";
  background-color: rebeccapurple;
  border-radius: 3px;
  border: none;
  color: white;
  font-weight: bold;
  padding: 15px 70px;
}
#anything {
  margin-top: 30px;
  padding-top: 30px;
}

