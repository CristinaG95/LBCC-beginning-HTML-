@import url("https://fonts.googleapis.com/css2?family=Itim&family=Knewave&display=swap");

h1 {
  font-family: "Knewave";
  font-size: 50px;
  text-align: center;
  background-color: olivedrab;
  padding: 20px;
  border-radius: 20px;
  margin: 20px 150px;
}
td,
h2,
p {
  font-family: "Itim";
}
nav {
  font-family: "Itim";
  text-align: center;
  font-size: 30px;
  background-color: darkolivegreen;
  margin: 8px;
  border-radius: 5px;
  padding: 10px;
  width: 150px;
}
a:visited,
a:link {
  text-decoration: none;
  color: white;
  font-weight: bold;
}
table {
  width: 900px;
  border-spacing: 10px;
}
td {
  color: white;
  font-size: 20px;
  padding: 30px;
  background-color: gray;
  border: solid olivedrab 15px;
  border-radius: 5px;
}
body {
  background-color: lightgray;
}
footer {
  font-family: Arial, Helvetica, sans-serif;
  color: black;
  border: solid 1px;
  padding: 10px;
  margin: 10px;
  text-align: center;
}


