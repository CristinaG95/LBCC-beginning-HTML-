*CSS 1*
@import url("https://fonts.googleapis.com/css2?family=Itim&family=Knewave&display=swap");

h1 {
  font-family: "Knewave";
  font-size: 50px;
  text-align: center;
  background-color: olivedrab;
  padding: 20px;
  border-radius: 20px;
  margin: 20px 150px;
}
ul {
  list-style: none;
  column-count: 3;
}
li {
  display: inline-block;
  width: 250px;
  background-color: slategrey;
  padding: 5px;
  margin: 8px;
  border-radius: 5px;
}
li:hover {
  filter: drop-shadow(8px 8px 10px gray);
}
a:visited,
a:link {
  text-decoration: none;
  font-family: "Itim";
  color: white;
}
nav {
  position: relative;
  top: 70px;
  left: 70%;
  font-family: "Itim";
  text-align: center;
  font-size: 30px;
  background-color: darkolivegreen;
  margin: 8px;
  border-radius: 5px;
  padding: 10px;
  width: 150px;
}
td,
p {
  font-family: "Itim";
  color: whitesmoke;
}
body {
  background-color: lightgray;
}
footer {
  font-family: Arial, Helvetica, sans-serif;
  color: black;
  border: solid 1px;
  padding: 10px;
  text-align: center;
}

